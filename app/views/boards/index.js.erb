 $(document).ready(function() {
    // Handle click event for "New Board" button
    $('#newBoardModalBtn').click(function() {
      $.ajax({
        url: '<%= new_board_path %>',
        type: 'GET',
        dataType: 'html',
        success: function(response) {
          $('#newBoardModalContent').html(response);
          $('#newBoardModal').modal('show');
        },
        error: function(xhr) {
          // Handle error if necessary
          console.error(xhr);
        }
      });
    });
  });
